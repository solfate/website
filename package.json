{
  "name": "solfate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn contentlayer && next dev",
    "build": "prisma generate && yarn contentlayer && next build",
    "start": "next start",
    "prisma:generate": "prisma generate",
    "contentlayer": "contentlayer build --clearCache",
    "db:org": "pscale org switch solfate",
    "db:connect": "yarn db:org && pscale connect solfate-main --port 3309 ",
    "db:branch": "yarn db:org && pscale branch create solfate-main ",
    "db:pr": "yarn db:org && pscale deploy-request create solfate-main ",
    "db:merge": "yarn db:org && pscale deploy-request deploy solfate-main ",
    "lint": "next lint"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.9.0",
    "@headlessui/react": "^1.7.19",
    "@mdx-js/react": "^3.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.11.0",
    "@solana-developers/helpers": "^1.3.1",
    "@solana/buffer-layout": "^4.0.1",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/spl-token": "^0.3.11",
    "@solana/spl-token-metadata": "^0.1.2",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-standard-features": "^1.1.0",
    "@solana/wallet-standard-util": "^1.1.0",
    "@solana/web3.js": "1.91.7",
    "add": "^2.0.6",
    "boring-avatars": "^1.10.2",
    "bs58": "^5.0.0",
    "clsx": "^2.1.1",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "fathom-client": "^3.6.0",
    "next": "14.2.3",
    "next-auth": "^4.24.5",
    "next-contentlayer": "^0.3.4",
    "next-mdx-remote": "^4.4.1",
    "react": "18.2.0",
    "react-dom": "18.3.1",
    "react-dom-confetti": "^0.2.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.4.1",
    "react-spinners": "^0.13.8",
    "solana-auth": "^0.0.10",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "dotenv": "^16.4.5",
    "eslint": "8.50.0",
    "eslint-config-next": "14.2.3",
    "octokit": "^3.2.0",
    "postcss": "8.4.38",
    "prisma": "^5.11.0",
    "tailwindcss": "3.4.3",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16"
  }
}
